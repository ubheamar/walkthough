<mvc:View
    xmlns:uxap="sap.uxap"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:f="sap.f"
    controllerName="ui5.walkthrough.controller.App"
>
<Shell>
    <App>
        <pages>
            <f:DynamicPage>
                <f:title>
                    <f:DynamicPageTitle>
                        <f:heading>
                            <Title text="{i18n>homePageTitle}" />
                        </f:heading>
                    </f:DynamicPageTitle>
                </f:title>
                <f:content>
                   <mvc:XMLView viewName="ui5.walkthrough.view.HelloPanel"></mvc:XMLView>
                </f:content>
            </f:DynamicPage>
        </pages>
    </App>
    </Shell>
</mvc:View>
